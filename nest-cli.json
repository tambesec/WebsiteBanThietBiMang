{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true
  },
  "watchOptions": {
    "ignored": [
      "node_modules",
      "dist",
      "client/**",
      "admin/**",
      "docker-export/**",
      "team-package/**",
      "*.log"
    ]
  }
}
